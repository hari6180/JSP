<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ page trimDirectiveWhitespaces="true" %>
<%
	/** 서블릿을 가정한 코드 부분 */
	// Beans객체를 request 객체에 추가하기
	Member member = new Member();
	member.setName("학생");
	member.setAge(20);
	request.setAttribute("my_member", member);
%>
%>
